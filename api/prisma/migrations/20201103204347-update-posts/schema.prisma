datasource DS {
  provider = ["sqlite", "postgresql"]
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String
  slug      String   @unique
  body      String
  image     String?
  createdAt DateTime @default(now())
}
